
value: dict
     | list
     | string
     | NUMBER  -> number
     | "true"  -> true
     | "false" -> false
     | "null"  -> null

list: "[" [value ("," value)*] "]"
dict: "{" [pair  ("," pair )*] "}" 
pair: string ":" value

string : ESCAPED_STRING

